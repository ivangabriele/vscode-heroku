{
  "name": "vscode-heroku",
  "version": "1.2.6",
  "displayName": "Heroku",
  "description": "Check your project's Heroku deployment status.",
  "main": "./out/extension",
  "license": "MIT",
  "engines": {
    "vscode": "1.36.1"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.vscode-heroku.linkWorkspace",
        "title": "Heroku: Link the current workspace to an existing Heroku app"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "pretest": "yarn compile",
    "test": "node ./out/test/runTest.js",
    "test:docker": "./scripts/run_tests_in_docker.sh",
    "preversion": "npm run compile",
    "postversion": "git push origin HEAD & git push --tags",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "await-to-js": "2.1.1",
    "command-exists": "1.2.8",
    "moment": "2.24.0"
  },
  "devDependencies": {
    "@types/fs-extra": "8.0.0",
    "@types/glob": "7.1.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.8",
    "@types/vscode": "1.36.0",
    "glob": "7.1.4",
    "mocha": "6.2.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3",
    "vscode-test": "1.0.2"
  },
  "author": {
    "name": "Ivan Gabriele",
    "email": "ivan.gabriele@gmail.com",
    "url": "https://www.ivangabriele.com"
  },
  "publisher": "ivangabriele",
  "icon": "https://github.com/ivangabriele/vscode-heroku/raw/master/res/icon.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "extension",
    "heroku",
    "visual studio code",
    "vsc",
    "vscode"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivangabriele/vscode-heroku.git"
  },
  "bugs": {
    "url": "https://github.com/ivangabriele/vscode-heroku/issues"
  },
  "homepage": "https://github.com/ivangabriele/vscode-heroku#readme"
}
